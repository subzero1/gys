        <property
            name="cl_id"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="cl_id"
        />

        <property
            name="clsl"
            type="java.lang.Double"
            update="true"
            insert="true"
            column="clsl"
        />

        <property
            name="de_id"
            type="java.lang.Integer"
            update="true"
            insert="true"
            column="de_id"
        />

        <property
            name="xh"
            type="java.lang.Integer"
